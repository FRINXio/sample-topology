                    <ipv4 xmlns="urn:huawei:yang:huawei-ip">
                        <addresses>
                            <address>
                                <ip>172.29.111.148</ip>
                                <mask>255.255.255.128</mask>
                                <type>main</type>
                            </address>
                        </addresses>
                    </ipv4>
                    <ipv6 xmlns="urn:huawei:yang:huawei-ip">
