                                <rate-limit>100</rate-limit>
                            </nd-vlan-car>
                            <nd-vlan-car>
                                <type>ns-unicast</type>
                                <rate-limit>100</rate-limit>
                            </nd-vlan-car>
                            <nd-vlan-car>
                                <type>ns-multicast</type>
                                <rate-limit>100</rate-limit>
                            </nd-vlan-car>
                        </nd-vlan-cars>
